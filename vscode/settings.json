{
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 12,
  "bladeFormatter.format.wrapLineLength": 60,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "editor.pasteAs.enabled": false,
  "editor.pasteAs.showPasteSelector": "never",
  "terminal.integrated.scrollback": 5000,
  "typescript.tsserver.log": "off",
  "tailwindCSS.classAttributes": ["class", "className", "classNames"],
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true, //=>
  "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
  "editor.tabSize": 2,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/bin/zsh"
    }
  },
  "cSpell.userWords": [
    "Abdl",
    "behaviour",
    "Buildable",
    "datetime",
    "denm",
    "dont",
    "fontawesome",
    "formatjs",
    "formik",
    "fortawesome",
    "getcanonicallocales",
    "gorhom",
    "gymflow",
    "gymflowmobile",
    "jsbundle",
    "languagedetector",
    "libphonenumber",
    "machine",
    "Mathf",
    "miragejs",
    "numberformat",
    "pawngame",
    "peasy",
    "pluralrules",
    "quadtree",
    "READMEABDL",
    "scenicness",
    "Sena",
    "state",
    "sweetalert",
    "waitlisted",
    "xcassets"
  ],
  "editor.suggestSelection": "first",
  "files.associations": {
    "*.html": "html",
    "*.pl": "prolog",
    "*.bdl": "bdl",
    "*.shader": "glsl"
  },
  "workbench.iconTheme": "material-icon-theme",
  "liveServer.settings.donotShowInfoMsg": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "cSpell.language": "en,pt-PT",
  "editor.rulers": [120],
  "explorer.confirmDelete": false,
  "terminal.integrated.tabs.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "editor.renderWhitespace": "none",
  "yaml.schemas": {
    "file:///Users/andrepatacas/.vscode/extensions/atlassian.atlascode-3.0.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml",
    "kubernetes": ["k8s.yaml"]
  },
  "redhat.telemetry.enabled": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Dracula",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "git.openRepositoryInParentFolders": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "nxConsole.showNodeVersionOnStartup": false,
  "[elixir]": {
    "editor.defaultFormatter": "animus-coop.vscode-elixir-mix-formatter"
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": [";"],
      "after": ["right"]
    },
    {
      "before": ["k"],
      "after": ["down"]
    },
    {
      "before": ["l"],
      "after": ["up"]
    },
    {
      "before": ["j"],
      "after": ["left"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-d>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-u>"]
    },
    {
      "before": ["<leader>", "c"],
      "after": [
        "v",
        "a",
        "\"",
        "c",
        "{",
        "c",
        "l",
        "a",
        "s",
        "s",
        "N",
        "a",
        "m",
        "e",
        "s",
        "(",
        ",",
        "{",
        "<Esc>","j",
        "j",
        "p"
      ],
    },
    {
      "before": ["g", "r"],
      "after": [],
      "commands": [{ "command": "editor.action.rename", "args": [] }]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [";"],
      "after": ["right"]
    },
    {
      "before": ["k"],
      "after": ["down"]
    },
    {
      "before": ["l"],
      "after": ["up"]
    },
    {
      "before": ["j"],
      "after": ["left"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-d>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-u>"]
    }
  ],
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineNumbers": "relative",
  "totalTypeScript.hiddenTips": [
    "generic-slots-in-functions",
    "basic-types",
    "typing-function-parameters",
    "passing-generics-to-types",
    "type-alias-declaration",
